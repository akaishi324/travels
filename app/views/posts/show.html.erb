<h1>Posts#show</h1>
<%= form_with(model: @post, local: true) do |f| %>
        <div class="form-group">
          <%= f.label :area, 'メッセージ' %>
          <%= f.text_area :area, class: 'form-control', rows: 5 %>
        </div>
        <div class="form-group">
          <%= f.label :country, 'メッセージ' %>
          <%= f.text_area :country, class: 'form-control', rows: 5 %>
        </div>
        <div class="form-group">
          <%= f.label :place, 'メッセージ' %>
          <%= f.text_area :place, class: 'form-control', rows: 5 %>
        </div>
        <div class="form-group">
          <%= f.label :comment, 'メッセージ' %>
          <%= f.text_area :comment, class: 'form-control', rows: 5 %>
        </div>
        <div class="field">
        <%= f.label :photo %>
        <%= f.file_field :photo %>
        </div>
<% end %>
<p><%= link_to 'edit',  edit_post_path(@post)%></p>
!form.select